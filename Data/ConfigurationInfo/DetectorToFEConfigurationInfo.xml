<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ConfigurationInfo.xsd">
  <CONFIGURATION Name="DetectorToFEConfiguration">    
    <VIEW Name="DETECTOR_TO_FE_CONFIGURATION" Type="File,Database,DatabaseTest">
      <COLUMN Type="UID"  		Name="DetectorID"              StorageName="DETECTOR_ID"                DataType="VARCHAR2"/>
      <COLUMN Type="Data" 		Name="FEWriterID"              StorageName="FE_WRITER_ID"               DataType="VARCHAR2"/>
      <COLUMN Type="Data" 		Name="FEWriterChannel"         StorageName="FE_WRITER_CHANNEL"          DataType="NUMBER"  />
      <COLUMN Type="Data" 		Name="FEWriterDetectorAddress" StorageName="FE_WRITER_DETECTOR_ADDRESS" DataType="NUMBER"  />
      <COLUMN Type="Data" 		Name="FEReaderID"              StorageName="FE_READER_ID"               DataType="VARCHAR2"/>
      <COLUMN Type="Data" 		Name="FEReaderChannel"         StorageName="FE_READER_CHANNEL"          DataType="NUMBER"  />
      <COLUMN Type="Data" 		Name="FEReaderDetectorAddress" StorageName="FE_READER_DETECTOR_ADDRESS" DataType="NUMBER"  />
      <COLUMN Type="Comment" 	Name="CommentDescription"   StorageName="COMMENT_DESCRIPTION"  	DataType="VARCHAR2"/>
      <COLUMN Type="Author" 	Name="Author"		           StorageName="AUTHOR"  	           		DataType="VARCHAR2"		/>
      <COLUMN Type="Timestamp" 	Name="RecordInsertionTime"     StorageName="RECORD_INSERTION_TIME" 		DataType="TIMESTAMP WITH TIMEZONE"/>
    </VIEW>
  </CONFIGURATION>
</ROOT>
